{
  "name": "setup-pleiades",
  "version": "1.0.0",
  "description": "Plieades Installer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack-all": "npm run pack-mac && npm run pack-win",
    "pack-mac": "electron-packager . --overwrite --platform=darwin --arch=x64",
    "pack-lin": "electron-packager . --overwrite --platform=linux --arch=x64",
    "pack-win": "electron-packager . --overwrite --platform=win32 --arch=ia32"
  },
  "author": "cypher256",
  "license": "EPL",
  "devDependencies": {
    "electron": "^1.8.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "fs-extra": "^4.0.2",
    "iconv-lite": "^0.4.19",
    "jquery": "^3.2.1"
  }
}
